{
  "name": "career-intelligence-pipeline",
  "version": "1.0.0",
  "description": "Career Intelligence ETL Pipeline - Node.js/TypeScript version for Railway deployment",
  "main": "dist/server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "db:setup": "tsx src/scripts/db-setup.ts",
    "db:prompts": "tsx src/scripts/ai-queries.ts",
    "onet:import": "tsx src/scripts/onet-import.ts",
    "worker:ai": "tsx src/scripts/ai-worker.ts",
    "verify": "tsx src/scripts/verify-and-reports.ts",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "career",
    "ai",
    "etl",
    "openai",
    "supabase",
    "postgres"
  ],
  "author": "Jeff",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "openai": "^4.24.1",
    "csv-parse": "^5.5.3",
    "ajv": "^8.12.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0"
  }
}
